<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="10dp"
        android:background="@drawable/housedetail_yuanjiao">

        <LinearLayout
            android:layout_width="match_parent"
            style="@style/height"
            android:layout_marginLeft="29dp"
            android:gravity="center">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/awb_detail_Mawb"
                android:textSize="@dimen/declare_size" />

            <TextView
                android:id="@+id/mawb_tv"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:background="@color/awb_detail_color"
                android:textColor="#2e2d33"
                android:textSize="@dimen/declare_size" />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#f4f4f4"
            android:layout_marginLeft="18dp"
            android:layout_marginRight="18dp"/>

        <LinearLayout
            android:layout_width="match_parent"
            style="@style/height"
            android:layout_marginLeft="29dp"
            android:gravity="center">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="代理代码:"
                android:textSize="@dimen/declare_size" />

            <TextView
                android:id="@+id/daili_daima_tv"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:background="@color/awb_detail_color"
                android:textSize="@dimen/declare_size"
                android:textColor="#2e2d33"/>
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#f4f4f4"
            android:layout_marginLeft="18dp"
            android:layout_marginRight="18dp"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="29dp"
            android:gravity="center">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                style="@style/longPage"
                android:text="代理名称:"
                android:textSize="@dimen/declare_size"/>

            <TextView
                android:id="@+id/dailimingcheng_tv"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                style="@style/longPage"
                android:layout_weight="2"
                android:background="@color/awb_detail_color"
                android:textSize="@dimen/declare_size"
                android:textColor="#2e2d33"/>
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#f4f4f4"
            android:layout_marginLeft="18dp"
            android:layout_marginRight="18dp"/>
        <LinearLayout
            android:layout_width="match_parent"
            style="@style/height"
            android:layout_marginLeft="29dp"
            android:gravity="center">

            <TextView
                android:id="@+id/textView10"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="启运港:"
                android:textSize="@dimen/declare_size" />

            <TextView
                android:id="@+id/qiyungang_tv"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:background="@color/awb_detail_color"
                android:textSize="@dimen/declare_size"
                android:textColor="#2e2d33"/>
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#f4f4f4"
            android:layout_marginLeft="18dp"
            android:layout_marginRight="18dp"/>
        <LinearLayout
            android:layout_width="match_parent"
            style="@style/height"
            android:layout_marginLeft="29dp"
            android:gravity="center">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="目的港:"
                android:textSize="@dimen/declare_size" />

            <TextView
                android:id="@+id/mudigang_tv"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:background="@color/awb_detail_color"
                android:textSize="@dimen/declare_size"
                android:textColor="#2e2d33"/>
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#f4f4f4"
            android:layout_marginLeft="18dp"
            android:layout_marginRight="18dp" />

        <LinearLayout
            android:layout_width="match_parent"
            style="@style/height"
            android:layout_marginLeft="29dp"
            android:gravity="center">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="总件数:"
                android:textSize="@dimen/declare_size"/>

            <TextView
                android:id="@+id/zongjianshu_tv"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:background="@color/awb_detail_color"
                android:textSize="@dimen/declare_size"
                android:textColor="#2e2d33"/>
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#f4f4f4"
            android:layout_marginLeft="18dp"
            android:layout_marginRight="18dp"/>
        <LinearLayout
            android:layout_width="match_parent"
            style="@style/height"
            android:layout_marginLeft="29dp"
            android:gravity="center">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="件数:"
                android:textSize="@dimen/declare_size" />

            <TextView
                android:id="@+id/pc_tv"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:background="@color/awb_detail_color"
                android:textSize="@dimen/declare_size"
                android:textColor="#2e2d33"/>
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#f4f4f4"
            android:layout_marginLeft="18dp"
            android:layout_marginRight="18dp"/>
        <LinearLayout
            android:layout_width="match_parent"
            style="@style/height"
            android:layout_marginLeft="29dp"
            android:gravity="center">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="重量:"
                android:textSize="@dimen/declare_size"/>

            <TextView
                android:id="@+id/weiight_tv"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:background="@color/awb_detail_color"
                android:textSize="@dimen/declare_size"
                android:textColor="#2e2d33"/>
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#f4f4f4"
            android:layout_marginLeft="18dp"
            android:layout_marginRight="18dp"/>
        <LinearLayout
            android:layout_width="match_parent"
            style="@style/height"
            android:layout_marginLeft="29dp"
            android:gravity="center">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="体积:"
                android:textSize="@dimen/declare_size" />

            <TextView
                android:id="@+id/volume_tv"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:background="@color/awb_detail_color"
                android:textSize="@dimen/declare_size"
                android:textColor="#2e2d33"/>
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#f4f4f4"
            android:layout_marginLeft="18dp"
            android:layout_marginRight="18dp"/>
        <LinearLayout
            android:layout_width="match_parent"
            style="@style/height"
            android:layout_marginLeft="29dp"
            android:gravity="center">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="品名:"
                android:textSize="@dimen/declare_size" />

            <TextView
                android:id="@+id/goods_tv"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:background="@color/awb_detail_color"
                android:textSize="@dimen/declare_size"
                android:textColor="#2e2d33"/>
        </LinearLayout>

    </LinearLayout>

</LinearLayout>